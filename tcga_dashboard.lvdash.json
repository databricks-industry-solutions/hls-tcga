{
  "datasets": [
    {
      "name": "465d0d69",
      "displayName": "cases_diagnoses",
      "queryLines": [
        "SELECT * FROM db_omics_solacc.tcga.cases_diagnoses"
      ]
    },
    {
      "name": "6810886a",
      "displayName": "cases_exposures",
      "queryLines": [
        "SELECT \n",
        "  e.alcohol_history, \n",
        "  e.years_smoked, \n",
        "  d.ethnicity, \n",
        "  d.gender, \n",
        "  d.race, \n",
        "  d.year_of_birth, \n",
        "  d.year_of_death,\n",
        "  f.primary_diagnosis,\n",
        "  CASE \n",
        "    WHEN d.year_of_death IS NOT NULL THEN d.year_of_death - d.year_of_birth\n",
        "    ELSE YEAR(current_date()) - d.year_of_birth\n",
        "  END AS age\n",
        "FROM db_omics_solacc.tcga.cases_exposures e\n",
        "JOIN db_omics_solacc.tcga.cases_demographics d\n",
        "  ON e.case_id = d.case_id\n",
        "LEFT JOIN db_omics_solacc.tcga.cases_diagnoses f \n",
        "  ON f.case_id = d.case_id\n",
        "WHERE f.primary_diagnosis IS NOT NULL"
      ],
      "catalog": "db_omics_solacc",
      "schema": "tcga"
    },
    {
      "name": "28ef8382",
      "displayName": "tsne_top",
      "queryLines": [
        "SELECT \n",
        "  t.*, \n",
        "  d.primary_diagnosis,\n",
        "  demo.gender\n",
        "FROM db_omics_solacc.tcga.tsne_top t\n",
        "LEFT JOIN db_omics_solacc.tcga.cases_diagnoses d\n",
        "  ON t.file_id = d.file_id\n",
        "LEFT JOIN db_omics_solacc.tcga.cases_demographics demo\n",
        "  ON d.case_id = demo.case_id"
      ]
    },
    {
      "name": "002a3e18",
      "displayName": "average_age",
      "queryLines": [
        "SELECT \n",
        "  f.primary_diagnosis,\n",
        "  d.race,\n",
        "  d.gender,\n",
        "  COUNT(*) AS n_cases,\n",
        "  AVG(\n",
        "    CASE \n",
        "      WHEN d.year_of_death IS NOT NULL THEN d.year_of_death - d.year_of_birth\n",
        "      ELSE YEAR(current_date()) - d.year_of_birth\n",
        "    END\n",
        "  ) AS avg_age\n",
        "FROM db_omics_solacc.tcga.cases_demographics d\n",
        "LEFT JOIN db_omics_solacc.tcga.cases_diagnoses f \n",
        "  ON f.case_id = d.case_id\n",
        "WHERE f.primary_diagnosis IS NOT NULL\n",
        "GROUP BY f.primary_diagnosis, d.race, d.gender\n",
        "ORDER BY n_cases DESC\n",
        "LIMIT 20"
      ]
    }
  ],
