{
  "lakehouse": {
    "catalog": "staging_catalog",
    "schema": "tcga_staging",
    "volume": "tcga_files_staging"
  },
  "api_paths": {
    "cases_endpt": "https://api.gdc.cancer.gov/cases",
    "files_endpt": "https://api.gdc.cancer.gov/files",
